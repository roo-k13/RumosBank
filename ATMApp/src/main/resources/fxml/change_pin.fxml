<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="300.0" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rumos.rumosbank.atmapp.controllers.ChangePinController">
    <PasswordField fx:id="insert_pin_text_field" promptText="Insert pin code" />
    <PasswordField fx:id="confirm_pin_text_field" promptText="Confirm pin code" />
    <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#onSavePinButtonClick" prefWidth="400.0" stylesheets="@../css/styles.css" text="Save" />
   <TextField fx:id="feedback_text_field" />
</VBox>
